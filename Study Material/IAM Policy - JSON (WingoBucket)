    "Version": "2023-03-06",
    "Statement": [
        {
            "Sid": "FullAcces",
            "Effect": "Allow",
            "Action": ["s3:*"],
            "Resource": ["*"]
        },
        {
            "Sid": "DenyWingoBucket",
            "Action": ["s3:"],
            "Effect": "Deny",
            "Resource": ["arn:aws:s3:::batmangifs", "arn:aws:s3:::batmangifs/*" ]
        }
    ]
